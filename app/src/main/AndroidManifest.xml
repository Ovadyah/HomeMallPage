<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.ovadyah.echome">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="com.ovadyah.echome.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.ovadyah.echome.demo1.ECHome1Activity"
            android:exported="false"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.ovadyah.echome.demo2.ECHome2Activity"
            android:exported="false"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.ovadyah.echome.demo3.ECHome3Activity"
            android:exported="false"
            android:screenOrientation="portrait"/>
    </application>

</manifest>